#Uncomment this line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, "10.0"
use_frameworks!
# ignore all warnings from all pods
inhibit_all_warnings!

def app_pods
 pod 'Alamofire'
 pod 'SwiftLint'
 pod 'RxSwift',    '~> 4.0'
 pod 'RxCocoa',    '~> 4.0'
 pod 'RxAlamofire'
 
end

# 광고 Pod


target "SwiftyDesignPattern" do
    app_pods
end

